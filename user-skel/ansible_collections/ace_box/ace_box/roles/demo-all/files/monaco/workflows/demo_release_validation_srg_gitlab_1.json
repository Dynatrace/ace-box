{
  "title": "{{ .name }}",
  "description": "",
  "tasks": {
    "trigger_good_build": {
      "name": "trigger_good_build",
      "description": "Trigger a GitLab pipeline",
      "action": "my.dynatrace.gitlab.tobias.gremmer:gitlab-trigger-pipeline",
      "input": {
        "name": "",
        "branchId": "main",
        "projectId": "{{ .projectId }}",
        "variables": [
          {
            "key": "BUILD_ID",
            "value": "1"
          }
        ],
        "connection": "{{ .connectionId }}",
        "branchInputType": 0,
        "projectInputType": 0
      },
      "position": {
        "x": 0,
        "y": 1
      },
      "predecessors": []
    }
  },
  "isPrivate": false,
  "trigger": {
    "schedule": {
      "rule": null,
      "trigger": {
        "type": "time",
        "time": "03:00"
      },
      "timezone": "Europe/Amsterdam",
      "isActive": true,
      "isFaulty": false,
      "filterParameters": {},
      "inputs": {}
    }
  },
  "schemaVersion": 3
}
