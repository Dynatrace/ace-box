apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
name: frontend-ingress
spec:
ingressClassName: "{{ ingress_class }}"
rules:
- host: "{{ hipstershop_domain }}"
    http:
    paths:
        - path: /
        pathType: Prefix
        backend:
            service:
            name: frontend-external
            port:
                number: 80